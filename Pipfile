[[source]]
url = "https://pypi.org/simple"
verify_ssl = true
name = "pypi"

[packages]
flet = "*"
asyncio = "*"
django = "*"
djangorestframework = "*"
python-dotenv = "*"
gunicorn = "*"
requests = "*"

[dev-packages]

[requires]
python_version = "3.12"

[scripts]
start = "python flet/main.py"
backend = "sh -c 'python manage.py migrate && gunicorn --workers 10 --timeout 60 -b 127.0.0.1:9501 pp.wsgi:application'"
flet = "flet -d -r flet/main.py"
runserver = "python manage.py runserver 0.0.0.0:8500"
android = "flet run -d -r flet/main.py --android"
dev = "'python manage.py runserver 0.0.0.0:8500' && 'flet -d -r flet/main.py'"
makemigrations = "python manage.py makemigrations"
migrate = "python manage.py migrate"
createsuperuser = "python manage.py createsuperuser"
startapp = "python manage.py startapp"
